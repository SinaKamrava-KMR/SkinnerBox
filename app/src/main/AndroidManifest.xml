<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mainway.skinnerbox">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_skinner_box"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_skinner_box_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SkinnerBox">
        <activity android:name=".Pages.Review.ReviewCards"></activity>
        <activity android:name=".Pages.AddCard.AddCard" />
        <activity android:name=".Pages.AddCard.EditCard" />
        <activity android:name=".Pages.Group.GroupPage" />
        <activity android:name=".Fragment.Pages.ViewCardsFragment" />
        <activity android:name=".Pages.Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>



</manifest>